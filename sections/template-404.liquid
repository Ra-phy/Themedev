<h1>{{ section.settings.main_title }}</h1>
<p>{{ section.settings.sub_title }}</p>


{% schema %}
  {
    "name": "Template 404",
    "settings": [
      {
        "type": "text",
        "id": "main_title",
        "default": "404",
        "label": "404 heading title"
      }, {
        "type": "text",
        "id": "sub_title",
        "default": "The page you are looking for does not exist",
        "label": "404 sub title"
      }
    ]
  }
{% endschema %}